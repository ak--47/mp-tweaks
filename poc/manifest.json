{
	"manifest_version": 3,
	"name": "Dynamic Header Modifier",
	"version": "1.0",
	"permissions": [
	  "declarativeNetRequest",
	  "declarativeNetRequestWithHostAccess"
	],
	"host_permissions": [
	  "*://*.testheaders.com/*"
	],
	"background": {
	  "service_worker": "background.js"
	},
	"action": {
	  "default_popup": "popup.html",
	  "default_icon": {

	  }
	}
  }
  